@ filename: init_clocks.s
@
@ Initializes all the clocks used for gpio, uart, timer.
@
@ Written by: James Ross


.text
.global _init_clocks
_init_clocks:

	STMFD SP! {R0-R10, LR}
	
	@ Enable GPIO1 clock	
	MOV R4, #CLK_EN	    	  @ R4 Holds mask for enableing clocks
	LDR R5, =GPIO1_CLK_CTRL
	STR R4, [R5]			  @ Store CLK_EN to enable GPIO1 clk
	
	@ Enable UART clock	
	
	LDMFD SP! {R0-R10,PC}